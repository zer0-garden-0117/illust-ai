{
  "name": "cfa-frontend",
  "version": "0.1.0",
  "private": true,
  "volta": {
    "node": "20.19.4",
    "yarn": "1.22.22"
  },
  "scripts": {
    "dev": "npm run bundleDoc && npm run genCode && next dev -p 3001 --experimental-https",
    "clean": "rm -rf build dist node_modules/.cache && node -e 'require(\"fs\").rmSync(\"src/generated\", { recursive: true, force: true })'",
    "build": "npm run bundleDoc && npm run genCode && next build",
    "start": "next start",
    "lint": "next lint",
    "clean:bundleDoc": "node -e 'require(\"fs\").rmSync(\"src/generated/services/cfa-v1-full.yaml\", { force: true })'",
    "bundleDoc": "npm run clean:bundleDoc && redocly bundle ../docs/openapi/v1/cfa-v1.yaml --output src/generated/services/cfa-v1-full.yaml",
    "clean:genCode": "node -e 'require(\"fs\").rmSync(\"src/generated/services/cfa-v1.d.ts\", { force: true })'",
    "genCode": "npm run clean:genCode && openapi-typescript src/generated/services/cfa-v1-full.yaml -o src/generated/services/cfa-v1.d.ts && node openapi-typescript/postTransform.js",
    "openapi-docker": "docker run --rm -p 8000:8080 -e SWAGGER_JSON=/openapi/cfa-v1-full.yaml -v $(pwd)/src/generated/services/cfa-v1-full.yaml:/openapi/cfa-v1-full.yaml swaggerapi/swagger-ui",
    "openapi-ui": "npm run bundleDoc && npm run openapi-docker"
  },
  "type": "module",
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.12",
    "@mantine/core": "^7.12.1",
    "@mantine/dates": "^7.12.1",
    "@mantine/dropzone": "^7.12.1",
    "@mantine/hooks": "^7.10.2",
    "@tabler/icons-react": "^3.7.0",
    "aws-amplify": "^6.3.8",
    "dotenv": "^16.4.5",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "mantine-react-table": "^2.0.0-beta.9",
    "next": "^14.2.28",
    "next-intl": "^3.15.3",
    "nextjs-google-analytics": "^2.3.7",
    "openapi-fetch": "^0.11.1",
    "openapi-typescript": "^7.3.0",
    "react": "^18",
    "react-circle-flags": "^0.0.23",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-world-flags": "^1.6.0",
    "swr": "^2.2.5",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@redocly/cli": "^1.11.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-world-flags": "^1.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
